        "def frequency_list_sum(list_of_elements):\n",
        "  counts={}\n",
        "  l=0\n",
        "  for k in list_of_elements:\n",
        "    l+=1\n",
        "    counts[k]=counts.get(k,0)+1\n",
        "  values=list(map(lambda x:x/l,list(counts.values())))\n",
        "  sos=sum(map(lambda x: x ** 2,values))\n",
        "  return sos"
